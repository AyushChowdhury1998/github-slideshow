

import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JTextField;
import javax.swing.JButton;
import javax.swing.JPasswordField;
import java.awt.event.ActionListener;
import java.awt.event.ActionEvent;
 
class hd implements ActionListener
{
	JFrame f;
	JLabel l1,l2,l3,l4;
	JTextField tf1,tf2;
	JButton b1;
	JPasswordField ps;
	                                      String uid[]= {"AYUSH","def456","ghi789"};//USER-ID
	                                      String upw[]= {"123abc","456def","789ghi"};//USER-PASSWORD
	
	
	hd()
	{
		f=new JFrame();
		f.setLayout(null);
		f.setTitle("WELLCOM TO HOTEL GR LOGIN PAGE :");
		f.setBounds(500,100,500,300);
		
		l1=new JLabel("USER-ID         :");
		l1.setBounds(10,70,120,40);
		tf1=new JTextField();
		tf1.setBounds(120,80,100,30);
	
		l2=new JLabel("PASSWORD :");
		l2.setBounds(10,120,100,40);
		ps=new JPasswordField();
		ps.setEchoChar('*');
		ps.setBounds(120,120,100,30);
		
		b1=new JButton("LOGIN");
		b1.setBounds(180,180,80,30);
		b1.addActionListener(this);
		
		l3=new JLabel("INCORRECT USER-ID OR PASSWORD !!");
		l3.setBounds(130,20,250,20);
		l3.setVisible(false);
		
		f.add(l1);
		f.add(tf1);
		f.add(l2);
		f.add(l3);
		f.add(b1);
		f.add(ps);
		f.setVisible(true);
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
	
	public void actionPerformed(ActionEvent ae)
	{
		int i=0,f=0;
		if(ae.getActionCommand().equalsIgnoreCase("LOGIN"))
		{
			
			String id="";
			String pw =" ";
			id=tf1.getText();
			pw=ps.getText();
			 
			for(i=0;i<uid.length;i++)
			{
			if(id.equalsIgnoreCase(uid[i]) && pw.equalsIgnoreCase(upw[i]))
			{
			f=1;
			break;
			}
			}
		if(f==1)
		{
			System.out.println(id);
			System.out.println(pw);
			
			list1 ob1=new list1(tf1.getText());
		}
		else
		{
			l3.setVisible(true);
		}
		}
	    }
		
	
public static void main(String args[])
{
	hd ob=new hd();
}
}

class list1 implements ActionListener
{
	JFrame f;
	JLabel l1,l2;
	JButton b1,b2,b3,b4,b5;
	String id="";
	list1(String id)
	{
		f=new JFrame();
		f.setLayout(null);
		f.setTitle("WELLCOM TO HOTEL ");
		f.setBounds(450,100,600,500);
	   
		l1=new JLabel("USER-ID :");
		l1.setBounds(100,20,100,30);
		
		l2=new JLabel(id);
		l2.setBounds(160,20,100,30);
		l2.setVisible(true);
		
	   b1=new JButton("TARIFF-LIST");
	   b1.setBounds(70,100,150,40);
	   b1.addActionListener(this);
	   
	   b2=new JButton("OCCUPANCY-LIST");
	   b2.setBounds(300,100,150,40);
	   b2.addActionListener(this);
	   
	   b3=new JButton("BOOKING-PAGE");
	   b3.setBounds(70,250,150,40);
	   b3.addActionListener(this);	
	   
	   b4=new JButton("BILL-GENERATE");
	   b4.setBounds(300,250,150,40);
	   b4.addActionListener(this);
	   
	   b5=new JButton("EXIT");
	   b5.setBounds(200,350,80,40);
	   b5.addActionListener(this);
	    f.add(l1); 
	    f.add(l2);
	    f.add(b1);
	    f.add(b2);
	    f.add(b3);
	    f.add(b4);
	    f.add(b5);
	    f.setVisible(true);
	    f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
	
public void actionPerformed(ActionEvent ae)
{
	if(ae.getActionCommand().equalsIgnoreCase("TARIFF-LIST"))
	{
		tariff ob2=new tariff();
	}
	else if(ae.getActionCommand().equalsIgnoreCase("OCCUPANCY-LIST"))
   {
		 occupancy ob3=new occupancy ();
   }
	else if(ae.getActionCommand().equalsIgnoreCase("BOOKING-PAGE"))
	{
		booking1 ob4=new booking1();
	}
	
	else if (ae.getActionCommand().equalsIgnoreCase("BILL-GENERATE"))
	{
		bill ob5=new bill();
	}
if(ae.getActionCommand().equalsIgnoreCase("EXIT"))
{
	login ob6=new login();
}
}
}

