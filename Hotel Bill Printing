	public void actionPerformed(ActionEvent ae)
	{
        float rpd,nbpd;
        int day[]= {0,31,28,31,30,31,30,31,31,30,31,30,31};
         int i;                                           
                                                          d1=Integer.parseInt(tf3.getText()+tf4.getText());
                                                          d2=Integer.parseInt(tf11.getText()+tf12.getText());
                                                          m1=Integer.parseInt(tf5.getText()+tf6.getText());
                                                          m2=Integer.parseInt(tf13.getText()+tf14.getText());
                                                          y1=Integer.parseInt(tf7.getText()+tf8.getText()+tf9.getText()+tf10.getText());
                                                          y2=Integer.parseInt(tf15.getText()+tf16.getText()+tf17.getText()+tf18.getText());
                                                          
        tr=Integer.parseInt(tf19.getText());
		r=Float.parseFloat(tf21.getText());
		gp=Float.parseFloat(gtf.getText());
		sev=Integer.parseInt(sf.getText());
        
		//(WHEN YEAR AND MONTHs ARE SAME BUT THE DAYS ARE DIIFERENT:)
		if(y1==y2 && m1==m2)
	    {
                       td=d2-d1;
	    }
		
		//(WHEN YEAR IS SAME BUT THE MONTHS AND DAYS ARE DIFFERENT : )
		else if (y1==y2 && m1<m2)
		{
			if(d1<day[m1])
			{
				td=td+(day[m1]-d1);
				for(i=m1+1;i<m2;i++)
				{
				td=td+day[i];
				}
				td=td+d2;
				}
			}
		
		//(WHEN YEAR AND MONTH AND DAY ARE DIFFERENT :)
		else if(y1!=y2 && y1<y2)
	    {
	    for(i=m1;i<=12;i++)
	    {
	    if(i==m1)
	    {
	    td=day[i]-d1;
	    }    
	    else
	    {
	    td=day[i];
	    }
	    }
	    
	    System.out.println("Breaking Days : " + td);
	    for(i=y1+1;i<y2;i++)
	    {
	    td=td+365;
	    }
	    for(i=1;i<m2;i++)
	    {
	    td=td+day[i];
	    }
	    td=td+d2;
	    }
		
		
		if(ae.getActionCommand().equalsIgnoreCase("VIEW-BILL :"))	
           {
	                   rpd=r+sev;
	                   g=(rpd*gp)/100;
	                   nbpd=rpd+g;
	                   gamt=g*td*tr;//
	                   nb=nbpd*td*tr;
	                   tf22.setText(Float.toString(gamt));
	                   tf23.setText(Float.toString(nb));
	                   tf24.setText(Integer.toString(td));
	                   System.out.println("NET FAIR PER DAY : " + nbpd );
	                   //System.out.println(" FAIR PER DAY without gst : " + );
}
		if(ae.getActionCommand().equalsIgnoreCase("VIEW-BILL :"))	
        {
	                   rpd=r+sev;
	                   g=(rpd*gp)/100;
	                   nbpd=rpd+g;
	                   gamt=g*td*tr;//
	                   nb=nbpd*td*tr;
	                   tf22.setText(Float.toString(gamt));
	                   tf23.setText(Float.toString(nb));
	                   tf24.setText(Integer.toString(td));
	                   System.out.println("NET FAIR PER DAY : " + nbpd );
	                   //System.out.println(" FAIR PER DAY without gst : " + );
}


if(ae.getActionCommand().equalsIgnoreCase("PRINT-BILL :"))
{
print ob2=new print(tf1.getText(),tf2.getText(),tf3.getText(),tf4.getText(),tf5.getText(),tf6.getText(),tf7.getText(),tf8.getText(),tf9.getText(),tf10.getText(),tf11.getText(),tf12.getText(),tf13.getText(),tf14.getText(),tf15.getText(),tf16.getText(),tf17.getText(),tf18.getText(),tf19.getText(),tf20.getText(),tf22.getText(),tf23.getText(),tf21.getText(),tf24.getText(),rnf.getText());	
}
}
    
	
public static void main(String args[])
{
bill ob=new bill();
}
}

