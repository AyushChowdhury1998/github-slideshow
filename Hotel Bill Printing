class login implements ActionListener
{
	JFrame f;
	JLabel l1,l2,l3,l4;
	JTextField tf1,tf2;
	JButton b1;
	JPasswordField ps;
	                                      String uid[]= {"AYUSH","def456","ghi789"};//USER-ID
	                                      String upw[]= {"123abc","456def","789ghi"};//USER-PASSWORD
	
	
	login()
	{
		f=new JFrame();
		f.setLayout(null);
		f.setTitle("WELLCOM TO HOTEL  LOGIN PAGE :");
		f.setBounds(500,100,500,300);
		
		l1=new JLabel("USER-ID         :");
		l1.setBounds(10,70,120,40);
		tf1=new JTextField();
		tf1.setBounds(120,80,100,30);
	
		l2=new JLabel("PASSWORD :");
		l2.setBounds(10,120,100,40);
		ps=new JPasswordField();
		ps.setEchoChar('*');
		ps.setBounds(120,120,100,30);
		
		b1=new JButton("LOGIN");
		b1.setBounds(180,180,80,30);
		b1.addActionListener(this);
		
		l3=new JLabel("INCORRECT USER-ID OR PASSWORD !!");
		l3.setBounds(130,20,250,20);
		l3.setVisible(false);
		
		f.add(l1);
		f.add(tf1);
		f.add(l2);
		f.add(l3);
		f.add(b1);
		f.add(ps);
		f.setVisible(true);
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
	
	public void actionPerformed(ActionEvent ae)
	{
		int i=0,f=0;
		if(ae.getActionCommand().equalsIgnoreCase("LOGIN"))
		{
			
			String id="";
			String pw =" ";
			id=tf1.getText();
			pw=ps.getText();
			 
			for(i=0;i<uid.length;i++)
			{
			if(id.equalsIgnoreCase(uid[i]) && pw.equalsIgnoreCase(upw[i]))
			{
			f=1;
			break;
			}
			}
		if(f==1)
		{
			System.out.println(id);
			System.out.println(pw);
			
			list1 ob1=new list1(tf1.getText());
		}
		else
		{
			l3.setVisible(true);
		}
		}
	    }
		
	
public static void main(String args[])
{
	//login ob6=new login();
}
}
